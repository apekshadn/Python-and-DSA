# logical operator
 #x<5 and x<10
 #x<5 or x<4
 #not(x<5 and x<10)

a=2
b=200
if a>b:
  print("a is greater than b")
elif a<b:
   print("b is greater than a")
else:
   print("both are equal")

name=input("Enter your name ")
age=int(input("Enter your age "))
if age>=18:
  print("You're eligible to vote")
else:
  print("You're not eligible to vote")
  c=18-age
  print("you will have to wait for",c,"years to vote")

name=input("Enter your name ")
age=int(input("Enter your age "))
if age>=18:
  print("You're eligible to vote")
else:
  print("You're not eligible to vote")

  print(f"you will have to wait for {18-age} years to vote")

# while loop
i=1
while i<6:
  print(i)
  i=i+1

  if i==3:
    continue


#for loop: it is a loop which is used for iterating over a sequence(list,tuple,etc)
fruits=["orange","apple","banana"]
for i in fruits:
  print(i)
